---
title: My Rad Blog
---

<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>{{ title }}</title>
  </head>
  <body>
    {{ content | safe }}

    Notes:

    <div>
      <ul>
      {%- for note in collections.note -%}
        <li{% if page.url == note.url %} aria-current="page"{% endif %}><a href="{{ note.url }}">{{ note.data.title }}</a></li>
      {%- endfor -%}
      </ul>
    </div>

  </body>
</html>